{% set icon_class = 'fa fa-file' %}

{% if 'workspace_' in log.getAction() %}
    {% set icon_class = 'fa fa-book' %}
{% endif %}

{% if 'user_' in log.getAction() %}
    {% set icon_class = 'fa fa-user' %}
{% endif %}

